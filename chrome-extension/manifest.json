{
  "manifest_version": 3,
  "name": "Smart Card ATR Reader",
  "version": "1.0",
  "description": "Test native messaging with smart cards",
  "permissions": [
    "nativeMessaging"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
